{
    "name": "joe-studio/tp5-login",
    "description": "基于TP5.024版本的管理后台登录功能",
    "keywords": ["thinkphp","login","extension"],
    "type": "tp5-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "JoeStudio",
            "email": "wstudio888@gmail.com"
        }
    ],
    "require": {
        "topthink/framework": "v5.0.24",
        "dh2y/think-crypt":"*"
    },
    
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "think\\": "library/think",
            "joeStudio\\login\\": "src/",
            "joeStudio\\login\\logic\\": "src/logic/",
            "joeStudio\\login\\helper\\": "src/helper/",
            "joeStudio\\login\\controller\\": "src/controller/",
            "joeStudio\\login\\model\\": "src/model/",
            "joeStudio\\login\\validate\\": "src/validate/",
            "joeStudio\\login\\config\\": "src/config/",
            "joeStudio\\login\\view\\": "src/view/",
            "joeStudio\\login\\behavior\\": "src/behavior/"
        }
    }
}
